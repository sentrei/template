    "critters": "^0.0.10",
    "gray-matter": "^4.0.3",
    "marked": "^3.0.4",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "next-compose-plugins": "^2.2.1",
    "next-mdx-remote": "^3.0.5",
    "next-og-utils": "^1.1.7",
    "next-seo": "^4.28.1",
    "next-sitemap": "^1.6.173",
    "next-themes": "^0.0.15",
    "next-translate": "^1.1.0",
    "postcss": "^8.3.8",
    "prisma": "3.1.1",
    "puppeteer-core": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.0",
    "react-twitter-widgets": "^1.10.0",
