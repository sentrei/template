  "devDependencies": {
    "@sentrei/commitlint": "^1.2.2",
    "@sentrei/depcheck": "^1.5.0",
    "@sentrei/dev": "^1.3.4",
    "@sentrei/eslint-config": "^1.5.8",
    "@sentrei/husky": "^1.3.0",
    "@sentrei/lint-staged": "^1.2.2",
    "@sentrei/npm-check-updates": "^1.5.1",
    "@sentrei/npm-package-json-lint-config": "^1.1.0",
    "@sentrei/prettier": "^1.5.1",
    "@sentrei/scripty": "^1.0.1",
    "@sentrei/semantic-release": "^1.5.3",
    "@sentrei/stylelint": "^1.2.0",
    "@sentrei/tailwindcss": "^1.4.3",
    "@sentrei/tsconfig": "^1.6.0",
    "@sls-next/serverless-component": "^3.4.0",
    "@types/cors": "^2.8.12",
    "@types/marked": "^3.0.1",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.4.3",
    "vercel": "^23.1.2",
    "webpack": "^5.56.0"
  },
